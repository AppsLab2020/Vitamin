<?xml version="1.0" encoding="UTF-8"?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       x:Class="Lekarnik.AppShell"
       xmlns:views="clr-namespace:Lekarnik.Views"
       x:Name="shell">

    

    <Tab Title="Priznaky" Icon="ill.png">
        <ShellContent ContentTemplate="{DataTemplate views:Priznaky}" Shell.TabBarBackgroundColor="Black" Shell.TabBarTitleColor="Green" Shell.DisabledColor="Red" Shell.UnselectedColor="Red"/>
       </Tab>
       <Tab Title="Vitaminy" Icon="vitamin.png" >
        <ShellContent ContentTemplate="{DataTemplate views:Vitaminy}" Shell.TabBarBackgroundColor="Black" Shell.TabBarTitleColor="Green" Shell.DisabledColor="Red" Shell.UnselectedColor="Red"/>
       </Tab>

        
</Shell>